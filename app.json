{
  "expo": {
    "name": "professional-mobile",
    "slug": "professional-mobile",
    "description": "professional-mobile is a platform for finding professionals in your area. offering all the services you need.",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/main-logo.png",
    "scheme": "professionals",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rezinio.professionals",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/main-logo.png",
        "backgroundImage": "./assets/images/main-logo.png",
        "monochromeImage": "./assets/images/main-logo.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.rezinio.professionalmobile"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/main-logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/main-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you upload proof for billing."
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "API_URL": "https://backend-ancient-water-2623.fly.dev",
      "router": {},
      "eas": {
        "projectId": "a31e7908-becf-475c-9829-38f73e1f6884"
      }
    }
  }
}
